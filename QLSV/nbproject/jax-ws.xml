    <service name="StudentWS">
      <implementation-class>soap.StudentWS</implementation-class>
    </service>
  </services>
  <clients>
    <client name="StudentWS">
      <wsdl-url>http://localhost:8080/QLSV/StudentWS?wsdl</wsdl-url>
